{
  "name": "chatBHP",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "bhp",
  "sourceRoot": "apps/chatBHP/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@angular/build:application",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/chatBHP-server/static/browser",
        "browser": "apps/chatBHP/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/chatBHP/tsconfig.app.json",
        "assets": [
          {
            "glob": "**/*",
            "input": "apps/chatBHP/public"
          }
        ],
        "styles": [
          "apps/chatBHP/src/styles.css"
        ]
      },
      "configurations": {
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/chatBHP/src/environments/environment.ts",
              "with": "apps/chatBHP/src/environments/environment.prod.ts"
            }
          ],
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "4kb",
              "maximumError": "8kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "optimization": false,
          "extractLicenses": false,
          "sourceMap": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "continuous": true,
      "executor": "@angular/build:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "chatBHP:build:production"
        },
        "development": {
          "buildTarget": "chatBHP:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular/build:extract-i18n",
      "options": {
        "buildTarget": "chatBHP:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "serve-static": {
      "continuous": true,
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "chatBHP:build",
        "staticFilePath": "dist/chatBHP-server/static/browser",
        "spa": true
      }
    }
  }
}
